#40. Rotate a tuple to the right by `n` positions.
def rotate_tuple_right(tup, n):
    if not tup:
        return tup  
    n = n % len(tup)  
    tup_list = list(tup)  
    rotated_list = tup_list[-n:] + tup_list[:-n]  
    return tuple(rotated_list) 

